    "build-pwa": "quasar build --mode pwa",
    "postinstall": "quasar prepare",
    "icon": "icongenie generate --icon src/assets/icon.png --quality 12 --svg-color F48120 && icongenie generate --icon src/assets/orange-cloud.png --quality 12 --filter splashscreen --svg-color F48120"
  },
